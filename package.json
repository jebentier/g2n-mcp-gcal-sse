{
  "name": "g2n-mcp-gcal-sse",
  "version": "1.1.0",
  "description": "Servidor MCP com SSE para Google Calendar",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "start": "node build/index.js",
    "dev": "ts-node-esm src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "docker:build": "docker build -t jebentier/g2n-mcp-gcal-sse:1.1.0 .",
    "docker:push": "docker push jebentier/g2n-mcp-gcal-sse:1.1.0",
    "docker:build-multi": "docker buildx build --platform linux/amd64,linux/arm64,linux/arm/v7 -t jebentier/g2n-mcp-gcal-sse:1.1.0 --push ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabriel-g2n/g2n-mcp-gcal-sse.git"
  },
  "keywords": [
    "mcp",
    "google-calendar",
    "sse",
    "docker",
    "n8n"
  ],
  "author": "Gabriel Augusto (@oaugustosgabriel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabriel-g2n/g2n-mcp-gcal-sse/issues"
  },
  "homepage": "https://github.com/gabriel-g2n/g2n-mcp-gcal-sse#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "express": "^5.0.1",
    "googleapis": "^148.0.0",
    "triple-beam": "^1.4.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
